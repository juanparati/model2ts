{
    "name": "juanparati/model2ts",
    "description": "An artisan command that can generate Typescript interfaces from Eloquent models.",
    "keywords": [
        "laravel",
        "helper",
        "typescript",
        "generator",
        "models",
        "interfaces"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/database": "^7 || ^8",
        "illuminate/console": "^6 || ^7 || ^8",
        "illuminate/filesystem": "^6 || ^7 || ^8",
        "illuminate/support": "^6 || ^7 || ^8",
        "doctrine/dbal": "~2.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5 || ^9"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.8-dev"
        },
        "laravel": {
            "providers": [
                "Juanparati\\Model2TS\\Model2TSServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\Model2Ts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\Model2Ts\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}